#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int string2int(char *str)
{
	int a;
	a=atoi(str);
	return a;
}

int main(void)
{
	int number;
	number=string2int("1234");
	if(number>=INT_MAX)
	{
		printf("It is too big\n");
		return 0;
	}
	else
		printf("number=%d\n",number);
		return 0;
}
